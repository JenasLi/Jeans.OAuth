@using Jeans.OAuth.Core.Domains;
@model List<UserEntity>
@{
    ViewBag.Title = "用户列表";
}

<a class="btn btn-primary" href="@Url.Action("UserAdd","OAuthGrant")" role="button">新增</a>
<div class="table-responsive">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>#</th>
                <th>Id</th>
                <th>用户名</th>
                <th>密   码</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <th scope="row">
                        <a class="btn btn-info btn-xs" href="@Url.Action("UserEdit","OAuthGrant",new { Id=item.Id})" role="button">编辑</a>
                        <a class="btn btn-danger btn-xs" href="@Url.Action("UserDelete","OAuthGrant",new { Id=item.Id})" role="button">删除</a>
                    </th>
                    <td>@item.Id</td>
                    <td>@item.UserName</td>
                    <td>@item.Password</td>
                </tr>
            }
        </tbody>
    </table>
</div>